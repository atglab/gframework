{
   "name": "GFManager",
   "indexHtmlPath": "index.html",
   "classpath": "${app.dir}/app",
   "overrides": "${app.dir}/overrides",
   "framework": "ext",
   "theme": "ext-theme-crisp-4fabb504-b411-44b1-b26f-b62bc28203ef",
   "requires": [
      "sencha-charts"
   ],
   "sass": {
      "namespace": "GFManager"
   },
   "js": [
